{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "next/core-web-vitals"
    ],
    "rules": {
        "react/display-name": "off",
        "no-unused-vars": [
            "warn",
            {
                "vars": "all",
                "args": "none",
                "ignoreRestSiblings": true
            }
        ],
        "no-unsafe-optional-chaining": "warn"
    }
}
